{
    "name": "calmforce/drupal4usa",
    "type": "project",
    "description": "Project template for Drupal 9 sites built with USWDS base theme.",
    "license": "GPL-2.0-or-later",
    "require": {
        "bower-asset/cropper": "^2.3",
        "bower-asset/slick-carousel": "^1.8",
        "cweagans/composer-patches": "^1.6.0",
        "drupal/core-composer-scaffold": "*",
        "oomphinc/composer-installers-extender": "^2.0",
        "drush/drush": "^10.5",
        "drupal/uswds_base": "^2.4",
        "drupal/config_split": "^1.6",
        "drupal/config_ignore": "^2.2",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/sqlsrv": "^4.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/address": "^1.9",
        "squizlabs/php_codesniffer": "^3.0.1",
        "drupal/coder": "^8.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/usfedgov_google_analytics": "^1.2",
        "drupal/tac_lite": "^1.5",
        "drupal/menu_item_extras": "^2.13",
        "drupal/field_group": "^3.1",
        "drupal/facets": "^2.0",
        "drupal/token_filter": "^1.2",
        "drupal/hierarchical_taxonomy_menu": "^1.42",
        "drupal/ds": "^3.12",
        "drupal/uswds_ckeditor_integration": "^1.0",
        "ext-json": "*",
        "drupal/structure_sync": "^2.0",
        "drupal/addtoany": "^1.15",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/simple_search_form": "^1.3",
        "ext-xml": "*",
        "drupal/core": "~9.5",
        "drupal/inline_entity_form": "^1.0-rc7",
        "drupal/page_manager": "^4.0-beta6",
        "drupal/panelizer": "^5.0-beta3",
        "drupal/pendo": "^1@alpha",
        "drupal/search_api": "^1.17",
        "drupal/metatag": "^1.16",
        "drupal/pathauto": "^1.8",
        "drupal/blazy": "^2.2",
        "drupal/entity_browser": "^2.5",
        "drupal/openapi_ui": "^1.0@RC",
        "drupal/openapi": "^2.0@RC",
        "drupal/crop": "^2.1",
        "drupal/diff": "^1.0",
        "drupal/layout_builder_restrictions": "^2.8",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/schemata_json_schema": "^1.0@beta",
        "drupal/schemata": "^1.0@beta",
        "drupal/redirect": "^1.6",
        "drupal/bg_image_formatter": "^1.14",
        "drupal/contact_storage": "^1.1",
        "drupal/entity_block": "^1.0@beta",
        "drupal/entity_browser_block": "^1.1",
        "drupal/image_widget_crop": "^2.3",
        "drupal/openapi_jsonapi": "^3.0",
        "drupal/openapi_ui_redoc": "^1.0@RC",
        "drupal/openapi_ui_swagger": "^1.0@RC",
        "drupal/simple_gmap": "^3.0",
        "drupal/slick": "^2.3",
        "drupal/slick_entityreference": "^2.0",
        "drupal/conflict": "^2.0@alpha",
        "wikimedia/composer-merge-plugin": "^2.0",
        "vardot/blazy": "1.8.2.1",
        "drupal/dropzonejs": "^2.5",
        "bower-asset/dropzone": "5.7.1",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "furf/jquery-ui-touch-punch": "dev-master",
        "drupal/like_and_dislike": "^1.0@beta",
        "drupal/govdelivery_signup-govdelivery_signup": "^2.0",
        "drupal/search_api_solr": "^4.2",
        "drupal/smtp": "^1.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/linkit": "^6.0@beta",
        "drupal/auditfiles": "^2.11",
        "drupal/views_data_export": "^1.0",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/slick_views": "^2.4",
        "drupal/insert_view": "^2.0",
        "drupal/focal_point": "^1.5",
        "drupal/uswds_paragraph_components": "^2.1",
        "drupal/uswds_blb_configuration": "^1.0@beta",
        "drupal/search_overrides": "^2.0",
        "drupal/extlink": "^1.6",
        "drupal/media_parent_entity_link": "1.0",
        "drupal/search_api_spellcheck": "^4.0",
        "drupal/search_api_autocomplete": "^1.5",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/section_library": "^1.0",
        "drupal/layout_builder_save_and_edit": "^1.0",
        "drupal/layout_builder_block": "^1.0",
        "drupal/facets_pretty_paths": "^1.2",
        "drupal/block_content_machine_name": "^3.0",
        "drupal/color_field": "^2.5",
        "symfony/deprecation-contracts": "2.5.2",
        "psr/cache": "1.0.1",
        "drupal/openid_connect": "^1.2",
        "drupal/group": "^3.0@beta",
        "ext-pdo": "*",
        "drupal/leaflet_more_markers": "^1.1",
        "drupal/leaflet_more_maps": "^2.1",
        "drupal/subgroup": "^3.0@beta"
    },
    "require-dev": {
        "zaporylie/composer-drupal-optimizations": "^1.1",
        "drupal/devel": "^4.0"
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": false,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset",
                "harvesthq/chosen"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "drupal-library"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/sqlsrv": {
                "3291199 - Call to a member function fetchField() on null": "https://www.drupal.org/files/issues/2022-06-18/sqlsrv-undefined-key-return-3291199-4.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "composer-patches": {
            "type": "vcs",
            "url": "https://github.com/vardot/composer-patches"
        },
        "jquery-ui-touch-punch": {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": "rm -r -f .travis.yml .travis-ci",
        "quick-start": [
            "composer install",
            "php docroot/core/scripts/drupal quick-start minimal --no-interaction"
        ]
    }
}
